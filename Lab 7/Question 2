#include <stdio.h>
int main() {
    char arr[100];
    printf("Enter a String: ");
    gets(arr);
    puts(arr);
    int counta=0;
    int counte=0;
    int counti=0;
    int counto=0;
    int countu=0;
    int i;
    for(i=0; i<100; i++){
        if(arr[i]=='a' || arr[i]=='A'){
            counta = counta + 1;
        }
        else if(arr[i]=='e' || arr[i]=='E'){
                counte = counte + 1;
            }
        else if(arr[i]=='i' || arr[i]=='I'){
                counti = counti + 1;
            }
        else if(arr[i]=='o' || arr[i]=='O'){
                counto = counto + 1;
            }
        else if(arr[i]=='u' || arr[i]=='U'){
                countu = countu + 1;
            }
    }
    printf("a = %d\n", counta);
    printf("e = %d\n", counte);
    printf("i = %d\n", counti);
    printf("o = %d\n", counto);
    printf("u = %d\n", countu);
    return 0;
}
