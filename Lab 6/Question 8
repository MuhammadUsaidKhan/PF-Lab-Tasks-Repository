#include <stdio.h>
int main() {
    int i;
    for(i=1; i<=5; i++){
        switch(i){
            case 1:
            printf("A B C D E\n");
            break;
            case 2:
            printf("  C   F  \n");
            break;
            case 3:
            printf("    E    \n");
            break;
            case 4:
            printf("  E   H  \n");
            break;
            case 5:
            printf("E F G H I\n");
            break;
        }
    }
    return 0;
}
