#include<stdio.h>
int main(){
	int num1;
	printf("Enter First Number: ");
	scanf("%d", &num1);
	int num2;
	printf("Enter Second Number: ");
	scanf("%d", &num2);
	char Operator;
	printf("Enter the Operator: ");
	scanf(" %c", &Operator);
	int result;
	switch(Operator){
		case '+':
			result = num1 + num2;
			printf("The Answer is: %d", result);
			break;
		case '-':
			result = num1 - num2;
			printf("The Answer is: %d", result);
			break;
		case '*':
			result = num1 * num2;
			printf("The Answer is: %d", result);
			break;
		default:
			result = num1/num2;
			printf("The Answer is: %d", result);
			
			
}
	return 0;
}
